package lista;

public class TarefaAdicionarElemento implements Runnable {

	private Lista lista;

	public TarefaAdicionarElemento(Lista lista, int numeroDoThread) {
		this.lista = lista;
	}

	@Override
	public void run() {
		for (int i = 0; i < 10; i++) {
			lista.adicionaElemento(" " + i);
		}
	}

}
